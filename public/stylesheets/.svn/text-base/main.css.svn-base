@charset "utf-8";
/* CSS Document */ * {
    margin: 0;
    padding: 0;
}

body {
    position: relative;
    width: 1000px;
    margin: 0 auto;
    font-size: 12px;
}

a {
    text-decoration: none;
    color: #000;
}

a:visited {
    color: #666;
}

a:hover {
    text-decoration: underline;    
}

.loginin {
    display: block;
    float: right;
    margin: 5px 0;
    font-family: Arial, sans-serif, Helvetica, Tahoma;
}

.loginin a {
    margin: 5px 0;
    padding: 0 8px;
    border-left: 1px solid #CCC;
}

.loginin .welcome {
    border-left: 0;
    color: #E28822;
}

.logo {
    margin-bottom: 5px;
    background: url("/images/logo.png") no-repeat scroll 50% 100% transparent color: #5585E2;
}

.mainContent {
    position: relative;
    display: inline;
    float: left;
    width: 768px;
}

.itemlist {
    display: inline;
    float: left;
    padding-right: 10px;
    width: 758px;
    text-align: left;
    border-right: 1px solid #EFEFEF;
}

.item {
    margin-bottom: 15px;
    padding-bottom: 5px;
    border-bottom: 1px solid #F3F3F3
}

.item .title {
    margin-bottom: 5px;
    font-size: 16px;
}

.item .title a {
    text-decoration: none;
    color: #0069CA;
}

.item .summary {
    font-size: 14px;
}

.item .date {
    font-size: 12px;
}

.rightColumn {
    position: relative;
    float: left;
    margin-left: 10px;
    width: 220px;
}

.righttitle {
    background-image: url("/images/logo2.gif");
    border-bottom: 1px solid #999;
    margin-top: 2px;
    font-size: 16px;
    color: #0069CA
}

.rightcontent {
    margin: 10px 0px;
    font-size: 14px;
    color: #000;
}

.rightcontent ul {
    padding: 0;
    margin: 0;
}

.rightcontent ul li {
    list-style-position: inside;
    list-style-type: disc;
}

.clear {
    clear: both;
    position: relative;
}

.footer {
    margin: 0 auto;
    margin-top: 50px;
    background: #ffffff;
    text-align: center;
    width: 1000px;
}

pre {
    background-color: .eee;
    padding:
    10px;
    font-size:
    11px;
}

div.field, div.actions {
    margin-bottom: 10px;
}

/* About Page */
.about_title{
	text-align: center;
}
.about_content{
	margin-top: 20px;
}
.about_content_para{
	font-size: 10.5pt;
	text-indent: 21pt;
	margin-bottom: 10px;
}
